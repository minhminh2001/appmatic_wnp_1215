<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:imaginato="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sv_slidemenu"
    android:layout_width="fill_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:paddingLeft="20dp"
    android:layout_gravity="start"
    android:orientation="vertical"
    android:id="@+id/menu_layout">
    <View android:id="@+id/v_top"
        android:layout_width="wrap_content"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:layout_marginTop="65dp"
        android:id="@+id/signin_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:id="@+id/signin_ImageView"
            android:layout_width="@dimen/slingmenu_width"
            android:layout_height="18dp"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="21dp"
            android:layout_marginRight="1dp"
            android:background="@mipmap/user" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:maxLines="2"
            android:ellipsize="end"
            android:text="@string/sign_register"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <!--<com.whitelabel.app.widget.CustomCircularImageView
            android:id="@+id/ccivUserAvater"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="11dp"
            android:visibility="gone" />
            -->
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/hassingin_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="18dp"
            android:layout_toRightOf="@+id/signin_ImageView"
            android:gravity="center_vertical"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            android:visibility="gone"
            style="@style/font_family_robot_regular"
            android:singleLine="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:background="@mipmap/bar"
        android:id="@+id/signin_home_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_RelativeLayout"
        android:layout_marginTop="30dp">
        <ImageView
            android:id="@+id/signin_home_ImageView"
            android:layout_width="18dp"
            android:layout_height="@dimen/slingmenu_width"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:background="@mipmap/v2_home" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_home_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/_home"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/signin_category_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_home_RelativeLayout">

        <ImageView
            android:id="@+id/signin_category_ImageView"
            android:layout_width="@dimen/slingmenu_width"
            android:layout_height="@dimen/slingmenu_width"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:background="@mipmap/v2_categorytree" />

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_category_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/category"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/tv_category_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/remind"
            android:layout_centerVertical="true"
            android:visibility="invisible"
            android:textSize="10sp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:gravity="center" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_shoppingCart_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_category_RelativeLayout">
        <ImageView
            android:id="@+id/signin_shoppingCart_ImageView"
            android:layout_width="18dp"
            android:layout_height="@dimen/slingmenu_width"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:background="@mipmap/shopping_cart" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_shoppingCart_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/shopping_cart"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/tv_shoppingCart_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/remind"
            android:layout_centerVertical="true"
            android:visibility="invisible"
            android:textSize="10sp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:gravity="center" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/signin_notification_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_shoppingCart_RelativeLayout">
        <ImageView
            android:id="@+id/signin_notification_ImageView"
            android:layout_width="15dp"
            android:layout_height="@dimen/slingmenu_width"
            android:layout_centerVertical="true"
            android:layout_marginLeft="22dp"
            android:scaleType="fitCenter"
            android:background="@mipmap/notification" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_notification_textview"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/notification"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/tv_notification_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/remind"
            android:layout_centerVertical="true"
            android:visibility="invisible"
            android:textSize="10sp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:gravity="center" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_creditCard_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_notification_RelativeLayout"
        android:visibility="gone">
        <ImageView
            android:id="@+id/signin_creditCard_ImageView"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24dp"
            android:scaleType="fitCenter"
            android:background="@mipmap/icon_storecredits" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_creditCard_textview"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:text="@string/sliding_credit_card"
            android:textColor="@color/greyC888888"
            android:gravity="center_vertical"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/signin_whishlist_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_creditCard_RelativeLayout">
        <ImageView
            android:id="@+id/signin_whishlist_ImageView"
            android:layout_width="@dimen/slingmenu_width"
            android:layout_height="15dp"
            android:layout_centerVertical="true"
            android:scaleType="fitCenter"
            android:layout_marginLeft="22dp"
            android:background="@mipmap/v2_wishlist" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_whishlist_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/my_wishlist"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/tv_whishlist_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/remind"
            android:layout_centerVertical="true"
            android:visibility="invisible"
            android:textSize="10sp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:gravity="center" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/signin_orders_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_whishlist_RelativeLayout">

        <ImageView
            android:id="@+id/signin_orsers_ImageView"
            android:layout_width="13dp"
            android:layout_height="@dimen/slingmenu_width"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24dp"
            android:background="@mipmap/order" />

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_orders_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/my_orders"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/tv_orsers_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/remind"
            android:layout_centerVertical="true"
            android:visibility="invisible"
            android:textSize="10sp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:gravity="center" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_address_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_orders_RelativeLayout">

        <ImageView
            android:id="@+id/signin_address_ImageView"
            android:layout_width="13dp"
            android:layout_height="@dimen/slingmenu_width"
            android:scaleType="fitCenter"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24dp"
            android:background="@mipmap/address" />

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_address_textview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:gravity="center_vertical"
            android:text="@string/my_address_book"
            android:textColor="@color/greyC888888"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/signin_storeCredit_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:visibility="gone"
        android:layout_below="@+id/signin_address_RelativeLayout">
        <ImageView
            android:id="@+id/signin_storeCredit_ImageView"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24dp"
            android:scaleType="fitCenter"
            android:background="@mipmap/icon_storecredits" />
        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_storeCredit_textview"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/slingmenu_text_height"
            android:layout_marginLeft="@dimen/slingmenu_text_marginleft"
            android:text="@string/store_credits"
            android:textColor="@color/greyC888888"
            android:gravity="center_vertical"
            android:textSize="17sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>




    <RelativeLayout
        android:id="@+id/signin_setting_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_storeCredit_RelativeLayout"
        android:layout_marginTop="30dp">

        <!--<ImageView-->
        <!--android:id="@+id/signin_setting_ImageView"-->
        <!--android:layout_width="22dp"-->
        <!--android:layout_height="22dp"-->
        <!--android:scaleType="fitCenter"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:layout_marginLeft="20dp"-->
        <!--android:src="@mipmap/sign_setting" />-->

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_setting_textview"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="22dp"
            android:gravity="center_vertical"
            android:text="@string/_settings"
            android:textColor="@color/greyC888888"
            android:textSize="15sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_customerService_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_setting_RelativeLayout">

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_customerService_textview"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="22dp"
            android:gravity="center_vertical"
            android:text="@string/customer_service"
            android:textColor="@color/greyC888888"
            android:textSize="15sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_helpcenter_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_customerService_RelativeLayout">

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_helpcenter_textview"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="22dp"
            android:gravity="center_vertical"
            android:text="@string/help_centre"
            android:textColor="@color/greyC888888"
            android:textSize="15sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/signin_shippingReturns_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/signin_helpcenter_RelativeLayout">

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/signin_shippingReturns_textview"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="22dp"
            android:gravity="center_vertical"
            android:text="@string/shipping_returns"
            android:textColor="@color/greyC888888"
            android:textSize="15sp"
            style="@style/font_family_robot_regular" />
    </RelativeLayout>

<!--
    <View
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/bg"
        android:background="@color/black80000000"
        android:visibility="gone"/>
        -->
</RelativeLayout>

</ScrollView>