<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:imaginato="http://schemas.android.com/apk/res-auto"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >
    <LinearLayout
        android:id="@+id/ll_toolbar"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/common_toolbar"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/greyEEEEEE"/>

    </LinearLayout>
    <ScrollView
        android:id="@+id/mScrollView"
        android:layout_below="@+id/ll_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/whiteFFFFFF"
        android:scrollbars="none">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/addaddress_ScrollView"
            android:clickable="true">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:id="@+id/edit_relative2"
                android:background="@color/whiteFFFFFF">

                <RelativeLayout
                    android:id="@+id/relative4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:focusable="true"
                    android:focusableInTouchMode="true">


                    <LinearLayout
                        android:id="@+id/ll_name2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/table_lable_margain_bottom"
                        android:orientation="horizontal">
                        <RelativeLayout
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                        <com.whitelabel.app.widget.CustomTextView
                            style="@style/font_family_robot_regular"
                            android:id="@+id/ctv_addaddress_firstName_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/table_margain_left"
                            android:textColor="@color/label_saved"
                            android:textSize="@dimen/labelTextFontSize"
                            android:text="@string/first_name"
                            android:visibility="invisible" />
                            <com.whitelabel.app.widget.CustomTextView
                                android:layout_below="@+id/ctv_addaddress_firstName_text2"
                                style="@style/font_family_robot_regular"
                                android:id="@+id/ctv_addaddress_firstName_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/table_margain_left"
                                android:text="@string/first_name"
                                android:textSize="@dimen/labelTextFontSize"
                                android:textColor="@color/colorAccent"
                                android:visibility="invisible" />
                            <RelativeLayout
                                android:layout_below="@+id/ctv_addaddress_firstName_text2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                                android:layout_marginLeft="@dimen/table_margain_left"
                                android:layout_weight="1">
                                <com.whitelabel.app.widget.CustomEditText
                                    style="@style/font_family_robot_regular"
                                    android:id="@+id/edit_addaddress_firstName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:hint="@string/first_name"
                                    android:paddingRight="@dimen/customEditText_paddingRight_v2_firstName"
                                    android:singleLine="true"
                                    android:textCursorDrawable="@null"
                                    android:textSize="@dimen/editTextFontSize"
                                    android:textColor="@color/editColor"
                                    android:textColorHint="@color/hint"
                                    android:imeOptions="actionNext"
                                    android:nextFocusForward="@+id/lastName"/>
                                <ImageView
                                    android:id="@+id/iv_addaddress_clear_first"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignTop="@+id/firstName"
                                    android:layout_centerVertical="true"
                                    android:background="@mipmap/text_elimination"
                                    android:layout_marginRight="2dp"
                                    android:visibility="gone"/>
                            </RelativeLayout>

                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        <com.whitelabel.app.widget.CustomTextView
                            style="@style/font_family_robot_regular"
                            android:id="@+id/ctv_addaddress_lastName_text2"
                            android:layout_width="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0dp"
                            android:textColor="@color/label_saved"
                            android:textSize="@dimen/labelTextFontSize"
                            android:text="@string/last_name"
                            android:visibility="invisible" />
                            <com.whitelabel.app.widget.CustomTextView
                                android:layout_below="@+id/ctv_addaddress_lastName_text2"
                                style="@style/font_family_robot_regular"
                                android:id="@+id/ctv_addaddress_lastName_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:text="@string/last_name"
                                android:textSize="@dimen/labelTextFontSize"
                                android:textColor="@color/colorAccent"
                                android:visibility="invisible" />
                            <RelativeLayout
                                android:layout_below="@+id/ctv_addaddress_lastName_text2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                                android:layout_marginLeft="5dp"
                                android:layout_weight="1">
                                <com.whitelabel.app.widget.CustomEditText
                                    style="@style/font_family_robot_regular"
                                    android:id="@+id/edit_addaddress_lastName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:hint="@string/last_name"
                                    android:paddingRight="@dimen/customEditText_paddingRight_v2"
                                    android:singleLine="true"
                                    android:textCursorDrawable="@null"
                                    android:textColorHint="@color/hint"
                                    android:textSize="@dimen/editTextFontSize"
                                    android:textColor="@color/editColor"
                                    />
                                <ImageView
                                    android:id="@+id/iv_addaddress_clear_last"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignTop="@+id/lastName"
                                    android:layout_centerVertical="true"
                                    android:background="@mipmap/text_elimination"
                                    android:visibility="gone"/>
                            </RelativeLayout>

                        </RelativeLayout>
                    </LinearLayout>



                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_height="wrap_content">
                        <View
                            android:id="@+id/view_firstname_line"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/line_height"
                            android:layout_marginTop="0dp"
                            android:background="@color/edittext_line" />
                        <View
                            android:id="@+id/view_lastname_line"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/line_height"
                            android:layout_marginTop="0dp"
                            android:background="@color/edittext_line" />
                    </LinearLayout>

                </RelativeLayout>

                <View
                    android:layout_centerHorizontal="true"
                    android:layout_width="@dimen/line_height"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/relative4"
                    android:layout_alignBottom="@+id/relative4"
                    android:background="@color/edittext_line"></View>

                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/relative4"
                    android:id="@+id/rl_addadd_country">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ctv_addaddresss_country_text2"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="@dimen/table_lable_margain_top">

                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_country"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:editable="false"
                            android:hint="@string/country"
                            android:paddingRight="20dp"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />

                        <ImageView
                            android:id="@+id/iv_country_arrow"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@mipmap/arrow_down_fill" />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/country"
                        android:layout_marginBottom="@dimen/table_lable_ani_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/ctv_addaddresss_country_text"
                        android:layout_below="@+id/ctv_addaddresss_country_text2"
                        android:visibility="invisible"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/country"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_country_text2"
                        android:visibility="visible"
                        android:editable="false"
                        android:textColor="@color/label_saved"
                        android:textSize="@dimen/labelTextFontSize"/>
                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>

                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_country"
                    android:id="@+id/rl_addadd_address1">
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_below="@+id/ctv_addaddresss_address1_text2">

                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_address1"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:hint="@string/address1"
                            android:paddingRight="@dimen/customEditText_paddingRight_v2"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />
                        <ImageView
                            android:id="@+id/iv_addaddress_clear_address1"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/text_elimination"
                            android:layout_alignParentRight="true"
                            android:visibility="gone"
                            />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/address1"
                        android:layout_below="@+id/ctv_addaddresss_address1_text2"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/ctv_addaddresss_address1_text1"
                        android:visibility="invisible"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/address1"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_address1_text2"
                        android:visibility="invisible"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/label_saved"/>
                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>
                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_address1"
                    android:id="@+id/rl_addadd_address2">

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ctv_addaddresss_address2_text2"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left">
                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_address2"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:hint="@string/address2_optional"
                            android:paddingRight="@dimen/customEditText_paddingRight_v2"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />

                        <ImageView
                            android:id="@+id/iv_addaddress_clear_address2"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:src="@mipmap/text_elimination"
                            android:visibility="gone" />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/address2_optional"
                        android:layout_below="@+id/ctv_addaddresss_address2_text2"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/ctv_addaddresss_address2_text1"
                        android:visibility="invisible"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/address2_optional"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_address2_text2"
                        android:visibility="invisible"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/label_saved"/>
                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>

                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_address2"
                    android:id="@+id/rl_addadd_postcode">
                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_below="@+id/ctv_addaddresss_postalcode_text2"
                        >

                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_postalcode"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:hint="@string/postal_code"
                            android:layout_marginRight="@dimen/customEditText_paddingRight_v2"
                            android:inputType="number"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />
                        <ImageView
                            android:id="@+id/iv_address_clear_code"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:src="@mipmap/text_elimination"
                            android:visibility="gone" />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/postal_code"
                        android:layout_below="@+id/ctv_addaddresss_postalcode_text2"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/ctv_addaddresss_postalcode_text"
                        android:visibility="invisible"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/postal_code"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_postalcode_text2"
                        android:visibility="invisible"
                        android:textColor="@color/label_saved"
                        android:textSize="@dimen/labelTextFontSize"/>
                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>
                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_postcode"
                    android:id="@+id/rl_addadd_city">

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ctv_addaddresss_city_text2"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="@dimen/table_lable_margain_top">

                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_city"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:hint="@string/city"
                            android:paddingRight="@dimen/customEditText_paddingRight_v2"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />

                        <ImageView
                            android:id="@+id/iv_address_clear_city"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:src="@mipmap/text_elimination"
                            android:visibility="gone" />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/city"
                        android:layout_below="@+id/ctv_addaddresss_city_text2"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textSize="@dimen/labelTextFontSize"
                        android:textColor="@color/colorAccent"
                        android:id="@+id/ctv_addaddresss_city_text1"
                        android:visibility="invisible"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/city"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_city_text2"
                        android:visibility="invisible"
                        android:textColor="@color/label_saved"
                        android:textSize="@dimen/labelTextFontSize"/>

                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>
                <com.whitelabel.app.widget.CustomButtomLineRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_city"
                    android:id="@+id/rl_addadd_state"
                    >

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ctv_addaddresss_state_text2"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="@dimen/table_lable_margain_top">

                        <com.whitelabel.app.widget.CustomEditText
                            android:id="@+id/edit_addaddresss_state"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:hint="@string/_state"
                            android:paddingRight="@dimen/customEditText_paddingRight_v2"
                            android:singleLine="true"
                            android:textColorHint="@color/hint"
                            android:textSize="@dimen/editTextFontSize"
                            android:textColor="@color/editColor"
                            android:textCursorDrawable="@null"
                            style="@style/font_family_robot_regular" />

                        <ImageView
                            android:id="@+id/iv_state_arrow"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@mipmap/arrow_down_fill" />
                    </RelativeLayout>

                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/_state"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_below="@+id/ctv_addaddresss_state_text2"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:textColorHint="@color/hint"
                        android:textSize="@dimen/editTextFontSize"
                        android:id="@+id/ctv_addaddresss_state_text1"
                        android:visibility="invisible"
                        />
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/_state"
                        android:layout_marginTop="@dimen/table_lable_margain_top"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:id="@+id/ctv_addaddresss_state_text2"
                        android:visibility="invisible"
                        android:textColor="@color/label_saved"
                        android:textSize="@dimen/labelTextFontSize"/>

                </com.whitelabel.app.widget.CustomButtomLineRelativeLayout>
                <RelativeLayout
                    android:id="@+id/relative13"
                    android:layout_width="fill_parent"

                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_addadd_state">


                    <com.whitelabel.app.widget.CustomTextView
                        android:id="@+id/ctv_addaddresss_phoneNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:text="@string/_phone"
                        android:visibility="gone"
                        android:textColor="@color/label_saved"
                        android:textSize="@dimen/labelTextFontSize"
                        style="@style/font_family_robot_regular" />

                    <com.whitelabel.app.widget.CustomTextView
                        android:id="@+id/ctv_addaddress_number_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ctv_addaddresss_phoneNumber"
                        android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                        android:layout_marginLeft="@dimen/table_margain_left"
                        android:layout_marginTop="8dp"
                        android:text="@string/add6"
                        android:visibility="gone"
                        android:textColor="@color/editColor"
                        android:textSize="@dimen/editTextFontSize"
                        style="@style/font_family_robot_regular" />
                    <View
                        android:layout_alignParentBottom="true"
                        android:layout_alignLeft="@+id/ctv_addaddresss_phoneNumber"
                        android:layout_alignRight="@+id/ctv_addaddresss_phoneNumber"
                        android:layout_width="wrap_content"
                        android:visibility="gone"
                        android:layout_marginRight="-3dp"
                        android:layout_height="@dimen/line_height"
                        android:background="@color/edittext_line" />
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/ctv_addaddresss_phoneNumber">
                        <View
                            android:layout_width="@dimen/line_height"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/rl_eg_layout"
                            android:visibility="gone"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="3dp"
                            android:layout_marginBottom="-10dp"
                            android:background="@color/edittext_line"></View>
                        <RelativeLayout
                            android:id="@+id/rl_eg_layout"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ctv_addaddresss_eg_text2"
                            android:layout_marginBottom="@dimen/table_lable_margain_bottom"
                            android:layout_marginTop="5dp"
                        >
                            <com.whitelabel.app.widget.CustomEditText
                                android:id="@+id/edit_addaddresss_eg"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:background="@null"
                                android:paddingRight="@dimen/customEditText_paddingRight_v2"
                                android:hint="@string/eg123"
                                android:maxLength="@integer/phone_number_length"
                                android:inputType="phone"
                                android:singleLine="true"
                                android:textColorHint="@color/hint"
                                android:textSize="@dimen/editTextFontSize"
                                android:textColor="@color/editColor"
                                android:textCursorDrawable="@null"
                                style="@style/font_family_robot_regular" />
                            <ImageView
                                android:id="@+id/iv_address_clear_phone"
                                android:layout_width="16dp"
                                android:layout_height="16dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:src="@mipmap/text_elimination"
                                android:visibility="gone" />
                        </RelativeLayout>

                        <com.whitelabel.app.widget.CustomTextView
                            android:id="@+id/ctv_addaddresss_eg_text2"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/eg123"
                            android:textColor="@color/label_saved"
                            android:textSize="@dimen/labelTextFontSize"
                            android:visibility="invisible"
                            style="@style/font_family_robot_regular" />

                        <com.whitelabel.app.widget.CustomTextView
                            android:id="@+id/ctv_addaddresss_eg_text"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/eg123"
                            android:textSize="@dimen/labelTextFontSize"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/colorAccent"
                            android:visibility="invisible"
                            style="@style/font_family_robot_regular"
                            android:layout_below="@+id/ctv_addaddresss_eg_text2"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentEnd="true"/>
                    </RelativeLayout>
                    <LinearLayout
                        android:layout_marginLeft="3dp"
                        android:layout_toRightOf="@+id/ctv_addaddresss_phoneNumber"
                        android:layout_width="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_height="wrap_content">

                        <View
                            android:id="@+id/v_add_phone_line"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="@dimen/line_height"
                            android:background="@color/edittext_line" />

                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relative14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/relative13"
                    android:layout_marginTop="20dp"
                    >
                    <!--<CheckBox-->
                        <!--android:id="@+id/addaddress_checkbox"-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:textColor="@color/grayText"-->
                        <!--android:checked="false" />-->
                    <com.whitelabel.app.widget.CustomCheckBox
                        android:id="@+id/addaddress_checkbox"
                        android:layout_centerVertical="true"
                        imaginato:color_checked="@color/colorAccent"
                        imaginato:color_unchecked_stroke="@color/color_checkbox_unchecked"
                        android:layout_width="24dp"
                        android:layout_height="24dp"/>
                    <com.whitelabel.app.widget.CustomTextView
                        style="@style/font_family_robot_regular"
                        android:layout_width="wrap_content"
                        android:id="@+id/tv_user_address"
                        android:layout_height="wrap_content"
                        android:text="@string/use_the_address_for_sipping"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/addaddress_checkbox"/>
                </RelativeLayout>

                <com.whitelabel.app.widget.CustomTextView
                    style="@style/font_family_robot_regular"
                    android:layout_below="@+id/relative14"
                    android:id="@+id/tv_error_hint"
                    android:gravity="center"
                    android:layout_marginTop="0dp"
                    android:textSize="15sp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:textColor="@color/red"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
    <ProgressBar
        android:id="@+id/pb_shoppingcart"
        android:layout_width="wrap_content"
        android:visibility="gone"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"/>
</RelativeLayout>