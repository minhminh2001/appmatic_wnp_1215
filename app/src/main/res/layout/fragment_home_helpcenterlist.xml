<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:imaginato="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/whiteffffff">

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/rlHeaderBar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/header_bar_height"
        android:background="@color/black000000"
        android:clickable="true">

        <RelativeLayout
            android:id="@+id/rlHeaderBarMenu"
            style="@style/HeaderBarLeftImageTouchTargetStyle">

            <ImageView
                android:id="@+id/vHeaderBarMenu"
                style="@style/HeaderBarLeftImageStyle"
                android:src="@drawable/action_menu"/>
        </RelativeLayout>

        <com.whitelabel.app.widget.CustomTextView
            android:id="@+id/ctvHeaderBarTitle"
            style="@style/HeaderBarTitleTextStyle"
            android:text="@string/home_helpcenter_title"/>
    </RelativeLayout>

    <!--<View-->
        <!--android:id="@+id/helpcenter_line"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="1dp"-->
        <!--android:layout_below="@id/rlHeaderBar"-->
        <!--android:background="@color/greye1e1e1"/>-->

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rlHeaderBar"
        android:scrollbars="vertical">

        <RelativeLayout
            android:id="@+id/scrollView_RelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/information_RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:background="@color/grayf8f8f8">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_information"
                    android:layout_width="wrap_content"
                    android:layout_height="42dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_information"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_roboto_medium"/>
            </RelativeLayout>

            <View
                android:id="@+id/information_line"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/information_RelativeLayout"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlAboutus"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/information_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_aboutus"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_aboutus"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_aboutus_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/aboutus_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlAboutus"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlPrivacypolicy"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/aboutus_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_privacypolicy"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_privacypolicy"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_privacypolicy_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/privacypolicy_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlPrivacypolicy"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlTermsofuse"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/privacypolicy_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_termsofuse"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_termsofus"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_termsofuse_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/termsofuse_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlTermsofuse"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/statement"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/termsofuse_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_statement"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/statement_of_ipr"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_statement_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/statement_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/statement"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/helpcenter1_RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:layout_below="@+id/statement_line"
                android:background="@color/grayf8f8f8">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_helpcenter"
                    android:layout_width="wrap_content"
                    android:layout_height="42dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_helpcenter"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_roboto_medium"/>
            </RelativeLayout>

            <View
                android:id="@+id/helpcenter1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/helpcenter1_RelativeLayout"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlHowtobuy"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/helpcenter1"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_howtobuy"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_howtobuy"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_howtobuy_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>


            <View
                android:id="@+id/howtobuy_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlHowtobuy"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlPayments"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/howtobuy_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_payments"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_payments"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_payments_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/payments_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlPayments"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlShippingdelivery"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/payments_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_shippingdelivery"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_shippingdelivery"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_shippingdelivery_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/shippingdelivery_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlShippingdelivery"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlOrdertracking"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/shippingdelivery_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_ordertracking"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_ordertracking"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_ordertracking_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/ordertracking_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlOrdertracking"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlCancellationsreturn"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/ordertracking_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <ImageView
                    android:id="@+id/helpcenter_cancellationsreturn_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@mipmap/arrow_right"/>

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_cancellationsreturns"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_cancellationsreturns"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="14dp"/>
            </RelativeLayout>

            <View
                android:id="@+id/cancellationsreturn_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlCancellationsreturn"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlGemcashvoucher"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/cancellationsreturn_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_gemcashvoucher"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_gemcashvoucher"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_gemcashvoucher_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/gemcashvoucher_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlGemcashvoucher"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/contactus_RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:layout_below="@+id/gemcashvoucher_line"
                android:background="@color/grayf8f8f8">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_contactus"
                    android:layout_width="wrap_content"
                    android:layout_height="42dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_contactus"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_roboto_medium"/>
            </RelativeLayout>

            <View
                android:id="@+id/contactus_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/contactus_RelativeLayout"
                android:background="@color/greye1e1e1"/>

            <RelativeLayout
                android:id="@+id/rlCustomerservice"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_below="@+id/contactus_line"
                android:background="@drawable/selector_helpcenterlist_and_notificationlist">

                <com.whitelabel.app.widget.CustomTextView
                    android:id="@+id/helpcenter_customerservice"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="center_vertical"
                    android:text="@string/home_helpcenter_list_customerservice"
                    android:textColor="@color/black000000"
                    android:textSize="16sp"
                    style="@style/font_family_robot_regular"/>

                <ImageView
                    android:id="@+id/helpcenter_customerservice_ImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="17dp"
                    android:src="@mipmap/arrow_right"/>
            </RelativeLayout>

            <View
                android:id="@+id/customerservice_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:layout_below="@+id/rlCustomerservice"
                android:background="@color/greye1e1e1"/>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
