<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="cpv_default_anim_autostart">false</bool>
    <bool name="cpv_default_is_indeterminate">false</bool>
    <color name="black000000">#000000</color>
    <color name="black80000000">#80000000</color>
    <color name="black_38">#60000000</color>
    <color name="black_54">#89000000</color>
    <color name="cpv_default_color">#2196F3</color>
    <color name="greyACACAC">#ACACAC</color>
    <color name="greyCBC9CC">#CBC9CC</color>
    <color name="greyE1E1E1">#E1E1E1</color>
    <color name="orangef2791e">#f2791e</color>
    <color name="transparent00">#00000000</color>
    <color name="whiteFAFAFA">#FAFAFA</color>
    <color name="whiteFFFFFF">#FFFFFF</color>
    <declare-styleable name="CircleImageView"><attr format="dimension" name="civ_border_width"/><attr format="color" name="civ_border_color"/><attr format="boolean" name="civ_border_overlay"/><attr format="color" name="civ_fill_color"/></declare-styleable>
    <declare-styleable name="CircularProgressView"><attr format="float" name="cpv_progress"/><attr format="float" name="cpv_maxProgress"/><attr format="integer" name="cpv_animDuration"/><attr format="integer" name="cpv_animSwoopDuration"/><attr format="integer" name="cpv_animSyncDuration"/><attr format="color" name="cpv_color"/><attr format="dimension" name="cpv_thickness"/><attr format="boolean" name="cpv_indeterminate"/><attr format="boolean" name="cpv_animAutostart"/><attr format="integer" name="cpv_animSteps"/><attr format="float" name="cpv_startAngle"/></declare-styleable>
    <declare-styleable name="CustomCheckBox"><attr format="integer" name="duration"/><attr format="dimension" name="stroke_width"/><attr format="color" name="color_tick"/><attr format="color" name="color_checked"/><attr format="color" name="color_unchecked"/><attr format="color" name="color_unchecked_stroke"/></declare-styleable>
    <declare-styleable name="CustomTextStyle"><attr name="textFont">
            <enum name="Default" value="-1"/>
        </attr></declare-styleable>
    <declare-styleable name="CustomViewPagerStyle"><attr format="boolean" name="isStopHorizontalScroll"/><attr format="boolean" name="isDisallowParentTouch"/></declare-styleable>
    <declare-styleable name="ParallaxScroll"><attr format="float" name="parallax_factor"/><attr format="float" name="alpha_factor"/><attr format="float" name="inner_parallax_factor"/><attr format="integer" name="parallax_views_num"/><attr format="boolean" name="circular_parallax"/></declare-styleable>
    <dimen name="cpv_default_thickness">4dp</dimen>
    <integer name="cpv_default_anim_duration">2500</integer>
    <integer name="cpv_default_anim_steps">3</integer>
    <integer name="cpv_default_anim_swoop_duration">5000</integer>
    <integer name="cpv_default_anim_sync_duration">500</integer>
    <integer name="cpv_default_max_progress">100</integer>
    <integer name="cpv_default_progress">0</integer>
    <integer name="cpv_default_start_angle">-90</integer>
    <string name="app_name">BaseFrameword</string>
    <string name="global_picker_three_cancel">Cancel</string>
    <string name="global_picker_three_done">Done</string>
    <style name="alertDialog">
        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@color/transparent00</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item>
        <!--模糊-->
    </style>
    <style mce_bogus="1" name="alertDialogAnim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>
    <style name="font_family_bold">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="font_family_normal">
    <!--Regular (default):-->
    <item name="android:fontFamily">sans-serif</item>
    <item name="android:textStyle">normal</item>
    </style>
    <style name="font_family_roboto_medium">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="font_family_roboto_regular">
        <!--Regular (default):-->
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="progressDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@color/transparent00</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item>
        <!--模糊-->
    </style>
</resources>